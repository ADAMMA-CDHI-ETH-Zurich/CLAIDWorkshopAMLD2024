{ "hosts": [
    {
          "hostname": "workshop_smartwatch",
          "type": "android",
          "is_server": true,
          "host_server_address": "192.168.1.78:1337",
          "min_log_severity_level": "DEBUG_VERBOSE",
          "modules": [
            {
              "type": "AccelerometerCollector",
              "id": "AccelerometerCollector",
              "input_channels": {},
              "output_channels" :
              {
                "AccelerationData" : "AccelerationData"
              },
              "properties" : {
                "samplingFrequency": "20",
                "outputMode": "STREAM"
              }
            }
          ]
        },
        {
          "hostname": "workshop_laptop",
          "type": "pc",
          "is_server": false,
          "connect_to": "workshop_smartwatch",
          "modules": [
            {
              "type": "HARModule",
              "id": "HARModule",
              "input_channels": {"InputData" :  "AccelerationData"},
              "output_channels": {"OutputData" : "ClassificationResult"}
            },
            {
              "type" : "DataSaverModule",
              "id" : "ClassificationSaver",
              "input_channels" : {"ClassificationResult" : "ClassificationResult"},
              "output_channels" : {},
              "properties":{
                "storagePath": "classifications",
                "fileNameFormat" : "test_data.json",
                "fileType" : "json",
                "what": "ClassificationResult"

              }
              
 
            },
            {
              "type": "AccelerationView",
              "id": "AccelerationView",
              "input_channels": {"InputData" :  "AccelerationData"},
              "output_channels": {}
            }
          ]
        }
    
      ]
    }